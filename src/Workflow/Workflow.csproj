<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net6.0</TargetFramework>

        <PackageId>byCrookie.Workflow</PackageId>

        <Title>Workflow</Title>
        <Authors>byCrookie</Authors>
        <PackageTags>workflow;core;async</PackageTags>
