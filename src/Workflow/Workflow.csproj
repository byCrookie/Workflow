    <ItemGroup>
      <PackageReference Include="bycrookie.nugetsdk" Version="1.0.6-pre-202203190600" />
      <PackageReference Include="JetBrains.Annotations" Version="2021.3.0" />
    </ItemGroup>

</Project>
